{
  "manifest_version": 2,
  "name": "GreyCord",
  "version": "1.0",
  "description": "Apply a greyscale filter to Discord.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.discord.com/*"],
      "js": ["greyscale.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
